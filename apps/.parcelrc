{
  "extends": "@parcel/config-default",
  "transformers": {
    "/*.{js,jsx,ts,tsx}": [
      "@parcel/transformer-babel",
      "@parcel/transformer-js"
    ],
    "*.{js,jsx,ts,tsx}": [
      "@parcel/transformer-babel",
      "@parcel/transformer-js"
    ],
    "*.yml": [
      "@parcel/transformer-yaml"
    ],
    "*.{jpg,png,svg}": ["@parcel/transformer-raw"],
    "*.{png,PNG}": ["@parcel/transformer-image"]
  }
}
